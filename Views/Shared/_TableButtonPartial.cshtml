@model WeddingApp.Models.IndividualButtonPartial
@using WeddingApp.Models


<td style="width:400px;">
	<div class="btn-group" role="group">
		@Html.Partial("_IndividualButtonPartial",
			new IndividualButtonPartial
			{
				Action = "Edit",
				ButtonType = "btn-primary",
				Glyph = "pencil",
				Text = "Edit Button",
				WeddingPackageId = Model.WeddingPackageId,
				EventId = Model.EventId,
				CustomerId = Model.CustomerId,
				FunctionId = Model.FunctionId,
				EnquiryCategoryId = Model.EnquiryCategoryId,
				EnquiryId = Model.EnquiryId
			})

		@Html.Partial("_IndividualButtonPartial",
			new IndividualButtonPartial
			{
				Action = "Delete",
				ButtonType = "btn-danger",
				Glyph = "trash",
				Text = "Delete Button",
				WeddingPackageId = Model.WeddingPackageId,
				EventId = Model.EventId,
				CustomerId = Model.CustomerId,
				FunctionId = Model.FunctionId,
				EnquiryCategoryId = Model.EnquiryCategoryId,
				EnquiryId = Model.EnquiryId
			})

		@Html.Partial("_IndividualButtonPartial",
			new IndividualButtonPartial
			{
				Action = "Details",
				ButtonType = "btn-success",
				Glyph = "list",
				Text = "Details Button",
				WeddingPackageId = Model.WeddingPackageId,
				EventId = Model.EventId,
				CustomerId = Model.CustomerId,
				FunctionId = Model.FunctionId,
				EnquiryCategoryId = Model.EnquiryCategoryId,
				EnquiryId = Model.EnquiryId
			})

	</div>
</td>

